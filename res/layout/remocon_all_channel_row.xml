<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/all_channel_row" android:layout_height="wrap_content" android:orientation="vertical" android:background="@drawable/list_bg" android:layout_width="fill_parent" android:soundEffectsEnabled="false">

	<LinearLayout android:layout_height="60dip"
		android:orientation="horizontal" android:gravity="center_vertical"
		android:layout_width="fill_parent" android:id="@+id/tvch_row_Layout" android:focusable="false" android:paddingRight="3dip" android:soundEffectsEnabled="false">
		<LinearLayout android:layout_height="fill_parent"
			android:layout_marginBottom="3dip" android:layout_marginTop="3dip"
			android:gravity="top|center_horizontal" android:orientation="vertical" android:focusable="false" android:layout_width="35dip">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textColor="@color/BLACK" android:id="@+id/TVCh_Number"
				android:textSize="17dip"
				android:layout_marginTop="3dip" android:text="23" android:focusable="false"></TextView>
			<ImageView android:background="@drawable/icon_mych"
				android:id="@+id/MyCh_Icon" android:layout_alignParentBottom="true"
				android:layout_marginBottom="3dip" android:layout_width="wrap_content" android:layout_height="wrap_content" android:focusable="false"></ImageView>
		</LinearLayout>
		<ImageView android:layout_height="wrap_content" android:focusable="false" android:layout_width="80dip" android:id="@+id/LogImage"></ImageView>
		<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:focusable="false" android:layout_weight="9" android:orientation="vertical">
			<TextView android:textSize="15dip" android:layout_height="wrap_content" android:focusable="false" android:singleLine="true" android:layout_width="fill_parent" android:layout_marginBottom="1dip" android:id="@+id/TVCh_Time" android:text="정보가 없습니다." android:textColor="@color/PLUM" android:ellipsize="marquee" android:marqueeRepeatLimit="marquee_forever"></TextView>
			<TextView android:textSize="11dip" android:layout_height="wrap_content" android:focusable="false" android:singleLine="true" android:layout_width="fill_parent" android:id="@+id/TVCh_NextTime" android:text="프로그램 정보가 없습니다." android:textColor="@color/BLACK" android:marqueeRepeatLimit="marquee_forever" android:ellipsize="marquee"></TextView>
		</LinearLayout>
		<ToggleButton android:id="@+id/tvch_list_icon_TglBtn" android:focusable="false" android:background="@drawable/list_icon" android:textOff="" android:textOn="" android:layout_height="wrap_content" android:layout_width="wrap_content" android:soundEffectsEnabled="false"></ToggleButton>
	</LinearLayout>
	
	<LinearLayout android:id="@+id/tvch_edit_Layout" android:layout_width="fill_parent" android:layout_height="35dip" android:paddingRight="40dip" android:paddingLeft="45dip" android:focusable="false" android:visibility="gone">
<ToggleButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/edit_button_register" android:textOn="" android:textOff="" android:layout_gravity="right|center" android:id="@+id/tvch_register_tglBtn" android:focusable="false" android:soundEffectsEnabled="false"></ToggleButton>
</LinearLayout>

	
</LinearLayout>
