<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:orientation="horizontal"
	android:gravity="center_vertical" android:layout_height="wrap_content"
	android:paddingLeft="10dip" android:paddingRight="10dip" android:soundEffectsEnabled="false">
	<LinearLayout android:layout_height="wrap_content"
		android:orientation="vertical" android:layout_width="fill_parent"
		android:layout_weight="90" android:soundEffectsEnabled="false">
		<TextView android:text="@+id/Lct_Name" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:textColor="@color/BLACK" android:id="@+id/Pct_Name" android:soundEffectsEnabled="false"></TextView>
	</LinearLayout>
	<RadioButton android:id="@+id/Pud_RBtn"
		android:layout_height="wrap_content" android:layout_width="40dip" android:focusable="false" android:clickable="false" android:soundEffectsEnabled="false"></RadioButton>

</LinearLayout>
